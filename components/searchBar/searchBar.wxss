/* components/searchBar/searchBar.wxss */
.searchBar {
  background-color: #42bd56;
  padding: 15rpx;
}

.searchNavigator {
  width: 100%;
  height: 60rpx;
  border-radius: 10rpx;
  background-color: #ffff;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 12%;
}

.searchInputGroup {
  width: 100%;
  height: 60rpx;
  border-radius: 10rpx;
  background-color: #ffff;
  display: flex;
}

.searchInputGroup .searchImage {
  width: 8%;
  height: 100%;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 50%;  
} 

.searchInputGroup .searchInput {
  width: 92%;
  height: 100%;
  font-size: 26rpx;
  color: #808080;
}

.searchPlaceholder{
  color: #808080;
}